$(document).ready(function () {

	$('#linkaddship').click(function () {
		$('#addship').slideDown('500')
	});

	$('#linkchangeship').click(function () {
		$('#changeship').slideDown('500')
	});

	$('#linkdeleteship').click(function () {
		$('#deleteship').slideDown('500')
	});
	alert("ok");

});


