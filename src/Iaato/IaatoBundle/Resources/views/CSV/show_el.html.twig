{% extends '::base.html.twig' %}

{% block title %}CSV uploaded{% endblock %}

{% block body %}
<div class="well">
<div class ="btn-group">
  <a class="btn btn-info" href={{ path('iaato_map_homepage') }}>Map</a>
  <a class="btn btn-info" href={{ path('iaato_step_list',{"index":"1"}) }}>List</a>
  <a class="btn btn-info" href={{ path('iaato_csv_download_steps_el') }}>Download Step CSV</a>
  <a class="btn btn-info" href={{ path('iaato_csv_upload_el') }}>Upload Step CSV</a>
  <a class="btn btn-info" href={{ path('iaato_csv_download_sites_el') }}>Sites</a>
</div>
	<table class="table table-hover table-bordered" style="width: 100%;background-color:white;margin-top:15px;">
		
		<thead style="background-color:#49afcd">
			<tr>
	  			<th style="color:iophyjkb"><center>{{ name }}</center></th>
			</tr>
		</thead>

		<tbody>
			<tr> <td>{{ cpt_done }} out of {{ cpt_total }} {{ name }} entered in the database !</td></tr>
      {% for err in error %}
        <tr> <td>{{ err }}</td> </tr>
      {% endfor %}
		</tbody>

	</table>
</div>
{% endblock %}
